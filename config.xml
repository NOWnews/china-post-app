<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ionic.conferenceapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>China Post</name>
    <description>
        An Ionic Framework application for the China Post App.
    </description>
    <author email="bird23074035@gmail.com" href="http://www.chinapost.com.tw/">
      China Post
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="tel:*" />
    <allow-navigation href="mailto:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/AppIcon24x24@2x.png" />
        <icon density="mdpi" src="resources/android/icon/AppIcon27.5x27.5@2x.png" />
        <icon density="hdpi" src="resources/android/icon/AppIcon40x40@2x.png" />
        <icon density="xhdpi" src="resources/android/icon/AppIcon44x44@2x.png" />
        <icon density="xxhdpi" src="resources/android/icon/AppIcon86x86@2x.png" />
        <icon density="xxxhdpi" src="resources/android/icon/AppIcon98x98@2x.png" />
        <icon src="resources/android/icon/AppIcon44x44@2x.png" />
        <splash density="port-mdpi" src="resources/android/splash/640.png" />
        <splash density="port-hdpi" src="resources/android/splash/960.png" />
        <splash density="port-xhdpi" src="resources/android/splash/1280.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/xxhdpi_1920x1920.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/xxxhdpi_2560x2560.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="orientation" value="default" />
        <icon height="57" src="resources/ios/icon/Icon.png" width="57" />
        <icon height="114" src="resources/ios/icon/Icon@2x.png" width="114" />
        <icon height="40" src="resources/ios/icon/Icon-Small-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/Icon-Small-40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/Icon-60@2x.png" width="120" />
        <icon height="50" src="resources/ios/icon/Icon-Small-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/Icon-Small-50@2x.png" width="100" />
        <icon height="60" src="resources/ios/icon/Icon-Notification@3x.png" width="60" />
        <icon height="120" src="resources/ios/icon/Icon-60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/Icon-60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/Icon-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/Icon-72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/Icon-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/Icon-76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/Icon-83.5@2x.png" width="167" />
        <icon height="29" src="resources/ios/icon/Icon-Small.png" width="29" />
        <icon height="58" src="resources/ios/icon/Icon-Small@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/Icon-Small@3x.png" width="87" />
        <splash height="2560" src="resources/ios/splash/xxxhdpi_2560x2560.png" width="2560" />
        <splash height="1920" src="resources/ios/splash/xxhdpi_1920x1920.png" width="1920" />
        <splash height="1280" src="resources/ios/splash/1280.png" width="1280" />
        <splash height="960" src="resources/ios/splash/960.png" width="960" />
        <splash height="640" src="resources/ios/splash/640.png" width="640" />
    </platform>
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="SplashScreen" value="screen" />
    <preference name="orientation" value="default" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="SplashScreenDelay" value="3000" />
    <engine name="android" spec="6.2.3" />
    <engine name="ios" spec="4.3.1" />
    <plugin name="cordova-plugin-console" spec="1.0.5" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~2.2.0">
        <variable name="XWALK_VERSION" value="22+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="1.1.4" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.4.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="2.2.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.1" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="https://github.com/ionic-team/cordova-plugin-wkwebview-engine.git" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
</widget>
